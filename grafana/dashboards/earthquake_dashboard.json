          "rawQuery": true,
          "rawSql": "SELECT\n  date_trunc('minute', event_time) AS \"time\",\n  COUNT(*) AS \"quakes_per_minute\"\nFROM earthquake_events\nWHERE event_time >= $__timeFrom()::timestamptz\n  AND event_time <= $__timeTo()::timestamptz\nGROUP BY 1\nORDER BY 1",
