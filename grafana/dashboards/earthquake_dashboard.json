          "datasource": { "type": "postgres", "uid": "postgresql-sink" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  latitude,\n  longitude,\n  CAST(magnitude AS float) AS magnitude,\n  place,\n  event_time,\n  ROUND(CAST(depth AS numeric), 1) AS depth_km\nFROM earthquake_events\nWHERE event_time >= NOW() - INTERVAL '24 hours'\n  AND latitude IS NOT NULL\n  AND longitude IS NOT NULL\nORDER BY event_time DESC\nLIMIT 2000",
          "refId": "A"
        }
