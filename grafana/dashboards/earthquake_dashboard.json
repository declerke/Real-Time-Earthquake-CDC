      "pluginVersion": "10.4.0",
      "targets": [
        {
          "datasource": { "type": "postgres", "uid": "postgresql-sink" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  CASE\n    WHEN place LIKE '%, %' THEN TRIM(SPLIT_PART(place, ', ', 2))\n    ELSE COALESCE(place, 'Unknown')\n  END AS region,\n  COUNT(*) AS quake_count,\n  ROUND(AVG(magnitude)::numeric, 2) AS avg_magnitude,\n  ROUND(MAX(magnitude)::numeric, 1) AS max_magnitude,\n  MIN(event_time) AS first_event,\n  MAX(event_time) AS last_event\nFROM earthquake_events\nWHERE event_time >= NOW() - INTERVAL '24 hours'\n  AND place IS NOT NULL\nGROUP BY 1\nORDER BY 2 DESC\nLIMIT 10",
          "refId": "A"
        }
      ],
      "title": "Top 10 Most Active Regions — Last 24 Hours",
      "type": "table"
